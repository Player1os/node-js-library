{
	"version": "0.2.0",
	"configurations": [{
		"name": "[Jest] Current functional file",
		"type": "node",
		"request": "launch",
		"program": "${workspaceFolder}\\node_modules\\jest\\bin\\jest",
		"args": [
			"-c", "./node_modules/@player1os/typescript-support/lib/jest/functional.js",
			"${relativeFile}"
		],
		"runtimeArgs": [
			"--nolazy",
			"-r", "@player1os/typescript-support/lib/runtime/build_on_change.js"
		],
		"cwd": "${workspaceRoot}",
		"console": "integratedTerminal",
		"internalConsoleOptions": "neverOpen",
		"protocol": "inspector",
		"windows": {
			"runtimeExecutable": "C:\\Programs\\nodejs\\node",
		}
	}, {
		"name": "[Jest] Current unit file",
		"type": "node",
		"request": "launch",
		"program": "${workspaceFolder}\\node_modules\\jest\\bin\\jest",
		"args": [
			"-c", "./node_modules/@player1os/typescript-support/lib/jest/unit.js",
			"${relativeFile}"
		],
		"runtimeArgs": ["--nolazy"],
		"cwd": "${workspaceRoot}",
		"console": "integratedTerminal",
		"internalConsoleOptions": "neverOpen",
		"protocol": "inspector",
		"windows": {
			"runtimeExecutable": "C:\\Programs\\nodejs\\node",
		}
	}]
}
